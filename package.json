{
  "name": "vs-dos",
  "description": "VS-DOS - MS-DOS games inside VSCode",
  "version": "2.0.0",
  "publisher": "ericson-willians",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EricsonWillians/vs-dos"
  },
  "keywords": [
    "oldschool",
    "ms-dos",
    "msdos",
    "90s",
    "nostalgic",
    "old",
    "doom",
    "doom2"
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewPanel:vsDos.run"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsDos.run",
        "title": "Start a DOS game",
        "category": "VS-DOS"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.73.0",
    "@types/vscode-webview": "^1.57.0",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "fetch-jsonp": "^1.2.3",
    "node-fetch": "^3.3.0",
    "xmldom": "^0.6.0"
  }
}
