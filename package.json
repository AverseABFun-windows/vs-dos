{
  "name": "vs-dos",
  "description": "VS-DOS - MS-DOS games inside VSCode",
  "version": "1.0.0",
  "publisher": "ericson-willians",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EricsonWillians/vs-dos"
  },
  "keywords": [
    "oldschool",
    "ms-dos",
    "msdos",
    "90s",
    "nostalgic",
    "old",
    "doom",
    "doom2"
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewPanel:vsDos.doom",
    "onWebviewPanel:vsDos.doom2",
    "onWebviewPanel:vsDos.finalDoom",
    "onWebviewPanel:vsDos.ultimateDoom"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsDos.doom",
        "title": "Start Doom",
        "category": "VS-DOS"
      },
      {
        "command": "vsDos.doom2",
        "title": "Start Doom2",
        "category": "VS-DOS"
      },
      {
        "command": "vsDos.finalDoom",
        "title": "Start Final Doom",
        "category": "VS-DOS"
      },
      {
        "command": "vsDos.ultimateDoom",
        "title": "Start Ultimate Doom",
        "category": "VS-DOS"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.73.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.9.4"
  }
}
